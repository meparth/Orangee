<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NoNeedHelpController">
   <children>
      <AnchorPane fx:id="anchor" onMouseDragged="#mouseDragged" onMousePressed="#mousePressed" prefHeight="382.0" prefWidth="603.0" style="-fx-background-color: #345;">
         <children>
            <Button fx:id="next_button" layoutX="305.0" layoutY="321.0" mnemonicParsing="false" onAction="#nextScene" style="-fx-background-color: #0000; -fx-border-color: #ccc; -fx-border-radius: 4;" text="proceed" textFill="#cccccc">
               <font>
                  <Font name="Merriweather Light" size="15.0" />
               </font>
            </Button>
            <Label layoutX="131.0" layoutY="78.0" text="Looks like you are okay!" textFill="#dddcdc">
               <font>
                  <Font name="Satisfy" size="36.0" />
               </font>
            </Label>
            <Label layoutX="62.0" layoutY="147.0" text="So if you want, you can help us help others." textFill="#c3c3c3">
               <font>
                  <Font name="Satisfy" size="30.0" />
               </font>
            </Label>
            <Label layoutX="43.0" layoutY="212.0" prefHeight="54.0" prefWidth="522.0" text="But if you want to check it out, you can take help anyway..." textFill="#c3c3c3">
               <font>
                  <Font name="Satisfy" size="23.0" />
               </font>
            </Label>
            <Button fx:id="next_button1" layoutX="170.0" layoutY="321.0" mnemonicParsing="false" onAction="#goServer" style="-fx-background-color: #0000; -fx-border-color: #ccc; -fx-border-radius: 4;" text="talk to others" textFill="#cccccc">
               <font>
                  <Font name="Merriweather Light" size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Button layoutX="516.0" layoutY="37.0" mnemonicParsing="false" onAction="#goHome" style="-fx-background-color: #0000;">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/home.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="cancel_button" layoutX="543.0" layoutY="36.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="20.0" prefWidth="4.0" style="-fx-background-color: #0000;">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/Delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
