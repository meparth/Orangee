<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NeedHelpController">
   <children>
      <AnchorPane fx:id="anchor" onMouseDragged="#mouseDragged" onMousePressed="#mousePressed" prefHeight="382.0" prefWidth="603.0" style="-fx-background-color: #345;">
         <children>
            <Button fx:id="next_button" layoutX="251.0" layoutY="329.0" mnemonicParsing="false" onAction="#nextScene" style="-fx-background-color: #0000; -fx-border-color: #ccc; -fx-border-radius: 4;" text="proceed" textFill="#cccccc">
               <font>
                  <Font name="Merriweather Light" size="15.0" />
               </font>
            </Button>
            <Label layoutX="113.0" layoutY="72.0" text="We think you are suffering" textFill="#dddcdc">
               <font>
                  <Font name="Satisfy" size="35.0" />
               </font>
            </Label>
            <Label layoutX="219.0" layoutY="201.0" text="Don't Worry!" textFill="#e1e1e1">
               <font>
                  <Font name="Satisfy" size="30.0" />
               </font>
            </Label>
            <Label layoutX="170.0" layoutY="240.0" text="We're here for you" textFill="#f8f8f8">
               <font>
                  <Font name="Satisfy" size="35.0" />
               </font>
            </Label>
            <Label layoutX="113.0" layoutY="115.0" text="from some mental pressure" textFill="#dddcdc">
               <font>
                  <Font name="Satisfy" size="35.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button layoutX="516.0" layoutY="37.0" mnemonicParsing="false" onAction="#goHome" style="-fx-background-color: #0000;">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/home.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="cancel_button" layoutX="543.0" layoutY="36.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="20.0" prefWidth="4.0" style="-fx-background-color: #0000;">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/Delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
